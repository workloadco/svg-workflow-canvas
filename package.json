    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.3.0",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "react-scripts-ts": "^3.1.0",
